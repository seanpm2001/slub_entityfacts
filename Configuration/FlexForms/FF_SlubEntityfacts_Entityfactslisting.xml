<T3DataStructure>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>EntityFacts Config</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
                    <settings.entityfacts.personality>
						<TCEforms>
							<label>Entity</label>
							<config>
								<type>input</type>
								<size>9</size>
								<eval>int</eval>
								<default>118607626</default>
							</config>
						</TCEforms>
					</settings.entityfacts.personality>
                    <settings.entityfacts.selection>
                        <label>Auswahl des Entitätstyps</label>
                        <onChange>reload</onChange>
                            <config>
                            <type>radio</type>
                            <items type="array">
                                <numIndex index="0" type="array">
                                <numIndex index="0">Persönlichkeit</numIndex>
                                <numIndex index="1">personality</numIndex>
                                </numIndex>
                                <numIndex index="1" type="array">
                                <numIndex index="0">Körperschaft</numIndex>
                                <numIndex index="1">corporation</numIndex>
                                </numIndex>
                                <numIndex index="3" type="array">
                                <numIndex index="0">Geografikum</numIndex>
                                <numIndex index="1">geographical</numIndex>
                                </numIndex>
                            </items>
                            </config>
                    </settings.entityfacts.selection>
                    <settings.entityfacts.facts>
                        <TCEforms>
                            <label>Was soll angezeigt werden?</label>
                            <displayCond>FIELD:settings.entityfacts.selection:=:personality</displayCond>
                            <config>
                                <type>select</type>
                                <renderType>selectMultipleSideBySide</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Name</numIndex>
                                        <numIndex index="1">preferredName</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Weiterer Name</numIndex>
                                        <numIndex index="1">variantName</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">In Beziehung stehende Person</numIndex>
                                        <numIndex index="1">relatedPerson</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">Biografisch/historische Angaben</numIndex>
                                        <numIndex index="1">biographicalOrHistoricalInformation</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">Beginn des Bestehens</numIndex>
                                        <numIndex index="1">dateOfEstablishment</numIndex>
                                    </numIndex>
                                    <numIndex index="5" type="array">
                                        <numIndex index="0">Ende des Bestehens</numIndex>
                                        <numIndex index="1">dateOfTermination</numIndex>
                                    </numIndex>
                                    <numIndex index="6" type="array">
                                        <numIndex index="0">Geburtsdatum</numIndex>
                                        <numIndex index="1">dateOfBirth</numIndex>
                                    </numIndex>
                                    <numIndex index="7" type="array">
                                        <numIndex index="0">Sterbedatum</numIndex>
                                        <numIndex index="1">dateOfDeath</numIndex>
                                    </numIndex>
                                    <numIndex index="8" type="array">
                                        <numIndex index="0">Verlinkungen auf weiterführende Webseiten</numIndex>
                                        <numIndex index="1">sameAs</numIndex>
                                    </numIndex>
                                    <numIndex index="9" type="array">
                                        <numIndex index="0">Abbildung</numIndex>
                                        <numIndex index="1">depiction</numIndex>
                                    </numIndex>
                                </items>    
                                <maxitems>20</maxitems>
                                <enableMultiSelectFilterTextfield>true</enableMultiSelectFilterTextfield>
                            </config>
                        </TCEforms>
                    </settings.entityfacts.facts> 



                </el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>  
